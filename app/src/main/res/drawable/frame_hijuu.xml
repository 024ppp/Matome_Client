<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">

    <!-- 背景色 -->
    <solid android:color="@color/hijuu" />

    <!-- サイズを指定 -->
    <size
        android:width="100dp"
        android:height="100dp"/>

    <!-- 枠線 width:線の幅、color:線の色 -->
    <stroke
        android:width="0.5dp"
        android:color="@color/black"
        />

</shape>